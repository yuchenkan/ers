'use strict'; /* vim: set ft=javascript: */

const base = goal.replace (/^live\//, '');

if (base.startsWith ('tst/') && await this.invoke ('live/tst/Goalfile') !== false) return;

if (new Set ([ 'convert.c.o', 'live.h', 'rtld' ]).has (base))
  return await this.invoke (`${goal}.g`);

return false;
